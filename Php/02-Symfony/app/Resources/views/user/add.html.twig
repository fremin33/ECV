{% extends 'base.html.twig' %}

{% block pageTitle %}
    <h1 class="display-4">Adding a new user!</h1>
{% endblock %}
{% block pageDescription %}
    <p class="lead">Fill in the form bellow.</p>
{% endblock %}

{% block body %}
	{{ form_start(form) }}
		{{ form_row(form.name) }}
		<button class="btn btn-success" type="submit"><i class="fas fa-plus fa-2x"></i></button>
	{{ form_end(form) }}
{% endblock %}